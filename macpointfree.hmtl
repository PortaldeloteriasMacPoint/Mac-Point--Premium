        }
        .menu-lateral button {
            display: block;
            width: 100%;
            padding: 10px;
            margin: 5px 0;
            background: white;
            border: 2px solid black ;
            border-radius: 5px;
            cursor: pointer;
           z-index: 1001;
          
        }
      
   .iframe-container {
            width: 100%;
            height: calc(100vh - 60px);
            position: absolute;
            top: 60px;
            left: 0;
            display: none;
        }
        .iframe-container iframe {
            width: 100%;
            height: 100%;
        }
  .menu-lateral {
  scrollbar-width: thin;
  scrollbar-color: White black ;
}

.menu-lateral::-webkit-scrollbar {

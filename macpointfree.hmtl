}


        .menu-lateral {
            width: 300px;
            background-color: white ;
            position: fixed;
            left: -300px;
            top: 80px;
           height: 90%;
            overflow-y: auto;
            transition: left 0.3s;
           padding: 10px;
            border-right: 5px solid white ;
            text-align: center;
          z-index: 1001;
          border: 2px solid black;
          text-shadow: 
  1px 2px 0px white , 
  -2px -2px 0px white, 
  2px -2px 0px white, 
  -2px 2px 0px white;  
        }
        .menu-lateral .foto-container {
            width: 120px;
            height: 120px;
            border-radius: 50%;
            border: 2px solid White;
            overflow: hidden;
            margin: 10px auto;
          z-index: 1001;
         text-shadow: 
2px 2px 0px black, 
  -2px -2px 0px black, 
  2px -2px 0px black, 
  -2px 2px 0px black;
}
 .menu-lateral img {
            width: 100%;
